{
  "name": "cgapis",
  "version": "0.0.8",
  "description": "Generate API code for any client and any server from the API spec file",
  "main": "dist/index.js",
  "bin": "dist/index.js",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "prepublishOnly": "npm run compile",
    "compile": "npm run clean && tsc -p .",
    "watch": "tsc -w -p .",
    "clean": "rm -rf dist",
    "start": "node dist/index.js --spec ./dist/input/api.json --server-dir  ./dist/output/server  --client-dir  ./dist/output/client  --api-version v1 --client-lang javascript --server-lang nodejs",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@types/fs-extra": "^5.0.5",
    "ajv": "^6.6.2",
    "array-operations": "^1.1.0",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.1",
    "commander": "^2.19.0",
    "compression": "^1.7.3",
    "deep-equal": "^1.0.1",
    "diff": "^3.5.0",
    "express": "^4.16.4",
    "fs-extra": "^7.0.1",
    "json-schema-to-typescript": "^6.1.0",
    "morgan": "^1.9.1",
    "prettier": "^1.15.3",
    "serve-favicon": "^2.5.0"
  },
  "devDependencies": {
    "@types/node": "^11.13.8",
    "typescript": "^3.4.5"
  },
  "keywords": [
    "api",
    "rpc",
    "web services",
    "code generator",
    "api generator",
    "codegen",
    "type-safe",
    "swagger",
    "json-schema",
    "javascript",
    "typescript",
    "http",
    "rest"
  ],
  "bugs": {
    "url": "https://github.com/HassanAlgoz/cgapis/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/HassanAlgoz/cgapis"
  },
  "author": "Hassan Algoz",
  "license": "MIT"
}
